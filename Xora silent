--> Silent Aim | Global Table <--
getgenv().Silent = {
    Settings = {
        Toggled = true,
        AimPart = "HumanoidRootPart", --> HumanoidRootPart, UpperTorso, LowerTorso, Head <-- Main Parts
        HitChance = 1000, 
        
        Prediction = {
            Enabled = true,
            Horizontal = 0.153, --> X, Z
            Vertical = 0.153, --> Y
        },
       
        Circle = {
            Visible = false,
            Color = Color3.fromRGB(238, 130, 238),
            Transparency = 0.3,
            Thickness = 1.5,
            NumSides = 1000,
            Radius = 150, --> Change Circle Size Here
            Filled = true,
        },
    },
}

